<?xml version="1.0"?>
<extension key="de.systopia.xdedupe" type="module">
  <file>xdedupe</file>
  <name>Extended Dedupe</name>
  <description>A powerful tool for large scale deduping</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.xdedupe</url>
    <url desc="Documentation">https://docs.civicrm.org/xdedupe/en/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.xdedupe/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-05-15</releaseDate>
  <version>1.3.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments>This tool was initially funded by Amnesty International Vlaanderen and ProVeg International</comments>
  <civix>
    <namespace>CRM/Xdedupe</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Xdedupe_Upgrader</upgrader>
</extension>
